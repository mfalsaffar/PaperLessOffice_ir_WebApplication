@model List<PaperLessOffice_ir_WebApplication.Models.Pro_GetMyJob_Result>

@{
    ViewBag.Title = "My Jobs";
}

<h2>My Jobs</h2>

<table class="table">
    <tr>
        <th>Fullname</th>
        <th>ProcName</th>
        <th>WfId</th>
        <!-- Add other headers as needed -->
    </tr>
    @foreach (var job in Model)
    {
        <tr>
            <td>@job.Fullname</td>
            <td>@job.ProcName</td>
            <td>@job.WfId</td>
            <!-- Add other columns as needed -->
        </tr>
    }
</table>
